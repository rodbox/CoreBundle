<table class="table">
	<thead>
		<tr>
			<th></th>
			{% for keyCol, col in data[0] %}
				<th class="col_{{ keyCol }}" data-col="{{ keyCol }}">
					{{ keyCol }}
				</th>
			{% endfor %}
		</tr>
	</thead>
	<tbody>
		{% for keyRow, row in data %}
			<tr>
				<td>
					<input id="row_{{ keyrow }}" type="checkbox" name="value[{{ keyRow }}]" value="{{ keyRow }}">
					<label for="">{{ keyRow }}</label>
				</td>
				{% for keyCol in col %}
					<td class="row_{{ keyRow }} col_{{ keyCol }}" data-row="{{ keyRow }}" data-col="{{ keyCol }}">
						<input type="text" class="form-control" name="value[{{ keyRow }}][{{ keyCol }}]" value="{{ col }}">
					</td>
				{% endfor %}
			</tr>
		{% endfor %}
	</tbody>
</table>